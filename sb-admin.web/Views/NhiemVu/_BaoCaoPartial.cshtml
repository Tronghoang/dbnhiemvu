
@using sb_admin.web.Models
@model List<BaoCao>
@for(int i= Model.Count-1; i>=0;i--)
{
    <div class="w3-row w3-border-bottom">
        <h3>Báo cáo lần @(i+1)</h3>
        <p>@Model[i].vMoTa</p>
        @if (Model[i].vDuongDanTapTin != null)
        {
            <a href="@Model[i].vDuongDanTapTin">Link tập tin</a>
        }
        @Html.Action("TapTinBaoCaoPartial", "NhiemVu", new { iMaBaoCaoCode = Model[i].iMaBaoCaoCode })
    </div>


}
